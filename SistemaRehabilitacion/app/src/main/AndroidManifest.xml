<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sistemarehabilitacion">

    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Vistas.Ejercicios.ConfigMalla"
            android:theme="@style/AppTheme.TemaPopup" />
        <activity
            android:name=".Vistas.Ejercicios.ConfiguracionPopup"
            android:theme="@style/AppTheme.TemaPopup" />
        <activity android:name=".Vistas.Musica.ListaReproduccion" />
        <activity android:name=".Vistas.Musica.ReproductoMusica" />
        <activity android:name=".Vistas.BaseDeDatos.SincronizacionActivity"></activity>
        <activity android:name=".Vistas.Errores.ErrorConexionBdRemota" />
        <activity android:name=".Vistas.Ejercicios.MenuActivity" />
        <activity android:name=".Vistas.GestionPacientes.Sesiones.ReportesActivity" />
        <activity android:name=".Vistas.GestionPacientes.Pacientes.EdicionActivity" />
        <activity android:name=".Vistas.GestionPacientes.Pacientes.RegistroActivity" />
        <activity android:name=".Vistas.GestionPacientes.Pacientes.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>